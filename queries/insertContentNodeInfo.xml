<query id="insertContentNodeInfo" action="insert">
    <tables>
        <table name="translation_content_node" />
    </tables>
    <columns>
		<column name="translation_content_node_srl" var="translation_content_node_srl" filter="number" notnull="notnull" />
		<column name="translation_project_srl" var="translation_project_srl" filter="number" notnull="notnull" />
		<column name="translation_file_srl" var="translation_file_srl" filter="number" notnull="notnull" />
        <column name="content_node" var="content_node" notnull="notnull" />
        <column name="lang" var="lang" notnull="notnull" />
		<column name="translation_count" var="translation_count" default="0" />
        <column name="recommend_count" var="recommend_count" default="0" />
    </columns>
</query>
